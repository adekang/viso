<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.viso.mapper.WordMapper">

    <select id="selectWord" resultType="com.example.viso.entity.WordEntity">
        SELECT name,value FROM word WHERE category=#{category}
    </select>
</mapper>